<h1>新規アカウント作成</h1>
<%= form_for :account, url:user_accounts_path do |f| %>
  <div class="">
    <%= f.label :name, "名前を入力してください" %>
  </div>
  <div class="">
    <%= f.text_field :name %>
  </div>
  <div class="">
    <%= f.label :value, "初期値を入力してください" %>
  </div>
  <div class="">
    <%= f.text_field :value %>
  </div>
  <div class="">
    <%= f.submit "アカウント作成" %>
  </div>
<% end %>

<ul class="nav-list">
  <li class="nav-list-item">
    <%= link_to "アカウント一覧へ戻る", user_accounts_path %>
  </li>
</ul>
