<h1>新規イベント作成</h1>
<%= form_for :events, url:user_events_path do |f| %>
  <div class="">
    <%= f.label :date, "日付を選択してください" %>
  </div>
  <div class="">
    <%= f.date_select :date, {} %>
  </div>
  <div class="">
    <%= f.label :genre, "ジャンルを選択してください" %>
  </div>
  <div class="">
    <%= f.select :genre, @genres_e, {include_blank: '選択ボックス'} %>
  </div>
  <div class="">
    <%= f.label :account, "アカウントを選択してください" %>
  </div>
  <div class="">
    <%= f.select :account, @accounts, {include_blank: '選択ボックス'} %>
  </div>
  <div class="">
    <%= f.label :memo, "メモ" %>
  </div>
  <div class="">
    <%= f.text_field :memo %>
  </div>
  <div class="">
    <%= f.label :value, "金額を入力してください" %>
  </div>
  <div class="">
    <%= f.text_field :value %>
  </div>
  <div class="">
    <%= f.submit "イベント登録" %>
  </div>
<% end %>
<div class="">
  <%= link_to "イベント一覧へ戻る", user_events_path %>
</div>
